{
	"name": "Timer"
  , "version": "1.0.5"
  , "manifest_version" : 2
  , "description": "A clean, simple countown timer with visual and audible alarm."
  , "background" : {
		"page" : "background.html"
	}
  , "browser_action" : {
		"default_icon": {
			"19": "icons/19.png",
			"38": "icons/38.png"
		},
		"default_title" : "Timer",
		"default_popup" : "fragments/popup.html"
	}
  , "permissions": [
    "notifications"
  ]
  , "web_accessible_resources": [
    "icons/48.png"
  ]
  , "icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	}
}
